{
  "name": "PiP Video",
  "version": "1.0.0",
  "description": "PiP video on browser.",
  "manifest_version": 3,
  "author": "Joey Clapton",
  "action": {
    "default_popup": "index.html",
    "default_title": "PiP Video"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
